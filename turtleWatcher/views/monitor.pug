extends  layout.pug 

block content 
  .row
  .col-12
    // interactive chart
    .card.card-primary.card-outline
      .card-header
        h3.card-title
          i.far.fa-chart-bar
          | Queue
      .card-body
        #interactive(style='height: 300px; padding: 0px; position: relative;')
          canvas.flot-base(width='1582' height='300' style='direction: ltr; position: absolute; left: 0px; top: 0px; width: 1582px; height: 300px;')
          canvas.flot-overlay(width='1582' height='300' style='direction: ltr; position: absolute; left: 0px; top: 0px; width: 1582px; height: 300px;')
          .flot-svg(style='position: absolute; top: 0px; left: 0px; height: 100%; width: 100%; pointer-events: none;')
            svg(style='width: 100%; height: 100%;')
              g.flot-x-axis.flot-x1-axis.xAxis.x1Axis(style='position: absolute; inset: 0px;')
                text.flot-tick-label.tickLabel(x='29.6875' y='293.75' style='position: absolute; text-align: center;') 0
                text.flot-tick-label.tickLabel(x='180.82954545454544' y='293.75' style='position: absolute; text-align: center;') 10
                text.flot-tick-label.tickLabel(x='336.2840909090909' y='293.75' style='position: absolute; text-align: center;') 20
                text.flot-tick-label.tickLabel(x='491.7386363636364' y='293.75' style='position: absolute; text-align: center;') 30
                text.flot-tick-label.tickLabel(x='647.1931818181818' y='293.75' style='position: absolute; text-align: center;') 40
                text.flot-tick-label.tickLabel(x='802.6477272727273' y='293.75' style='position: absolute; text-align: center;') 50
                text.flot-tick-label.tickLabel(x='958.1022727272727' y='293.75' style='position: absolute; text-align: center;') 60
                text.flot-tick-label.tickLabel(x='1113.5568181818182' y='293.75' style='position: absolute; text-align: center;') 70
                text.flot-tick-label.tickLabel(x='1269.0113636363635' y='293.75' style='position: absolute; text-align: center;') 80
                text.flot-tick-label.tickLabel(x='1424.465909090909' y='293.75' style='position: absolute; text-align: center;') 90
              g.flot-y-axis.flot-y1-axis.yAxis.y1Axis(style='position: absolute; inset: 0px;')
                text.flot-tick-label.tickLabel(x='9.625' y='268.25' style='position: absolute; text-align: right;') 0
                text.flot-tick-label.tickLabel(x='1' y='16.25' style='position: absolute; text-align: right;') 60
                text.flot-tick-label.tickLabel(x='1' y='226.25' style='position: absolute; text-align: right;') 10
                text.flot-tick-label.tickLabel(x='1' y='184.25' style='position: absolute; text-align: right;') 20
                text.flot-tick-label.tickLabel(x='1' y='142.25' style='position: absolute; text-align: right;') 30
                text.flot-tick-label.tickLabel(x='1' y='100.25' style='position: absolute; text-align: right;') 40
                text.flot-tick-label.tickLabel(x='1' y='58.25' style='position: absolute; text-align: right;') 50
  script(src='./public/js/monitor.js')
    